    Private Sub MyDash_VisibleChanged(sender As Object, e As EventArgs) Handles MyBase.VisibleChanged

        '----------- notifications warning ----------------
        Dim new_emails As Boolean : new_emails = False

        Try
            Dim cmd4 As New MySqlCommand
            cmd4.Parameters.AddWithValue("@receiver", current_user)
            cmd4.CommandText = "SELECT * from management.Dropbox where receiver = @receiver and read_m = 'n'"
            cmd4.Connection = Login.Connection
            Dim reader4 As MySqlDataReader
            reader4 = cmd4.ExecuteReader

            If reader4.HasRows Then
                While reader4.Read
                    new_emails = True
                End While
            End If

            reader4.Close()

        Catch ex As Exception
            MessageBox.Show(ex.ToString)
        End Try

        If new_emails = True Then

            Label6.BackColor = Color.Firebrick
            Label6.Text = "New Messages"

        Else
            Label6.BackColor = Color.FromArgb(61, 60, 78)
            Label6.Text = "Notifications"
        End If



    End Sub

